config_version=5

_global_script_classes=[{
"base": "Control",
"class": &"BaseMapod4dUi",
"language": &"GDScript",
"path": "res://mapod4d_core/base_mapod4d_ui/base_mapod4d_ui.gd"
}, {
"base": "Control",
"class": &"Mapod4dMain",
"language": &"GDScript",
"path": "res://mapod4d_core/mapod4d_main/mapod4d_main.gd"
}, {
"base": "Node3D",
"class": &"Mapod4dSphericalMetaverse",
"language": &"GDScript",
"path": "res://mapod4d_core/base_metaverses/mapod4d_spherical_metaverse/mapod4d_spherical_metavese.gd"
}]
_global_script_class_icons={
"BaseMapod4dUi": "",
"Mapod4dMain": "",
"Mapod4dSphericalMetaverse": ""
}

[application]

config/name="MAPOD4D Multiverse Core V2"
config/description="Multiverse of metaverse MAPOD4D"
run/main_scene="res://mapod4d_core/mapod4d_main/mapod4d_main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Mapod4dAutoload="*res://mapod4d_core/mapod4d_autoload/mapod4d_autoload.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
